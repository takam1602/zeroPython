      "source": [
        "import ast \n",
        "from graphviz import Digraph\n",
        "\n",
        "def visit(node,nodes,pindex,g):\n",
        "  name = str(type(node).__name__)\n",
        "  index = len(nodes)\n",
        "  nodes.append(index)\n",
        "  g.node(str(index),name)\n",
        "  if index != pindex:\n",
        "    g.edge(str(pindex),str(index))\n",
        "  \n",
        "  for n in ast.iter_child_nodes(node):\n",
        "    visit(n,nodes,index,g)\n",
        "\n",
        "\n",
        "def show_ast(src):\n",
        "  graph = Digraph()\n",
        "  tree = ast.parse(src)\n",
        "  visit(tree,[],0,graph)\n",
        "  return graph"
      ],
      "execution_count": 8,
      "outputs": []
