      "cell_type": "code",
      "metadata": {
        "id": "O1Y0Ar8-3WJk"
      },
      "source": [
        "def calc(code):\n",
        "  data = code.split()\n",
        "  stack = []\n",
        "  for x in data:\n",
        "    print(stack,x,end=\" =>\")\n",
        "    if x== '+':\n",
        "      b= stack.pop()\n",
        "      a = stack.pop()\n",
        "      stack.append(a+b)\n",
        "    elif x == '-':\n",
        "      b = stack.pop()\n",
        "      a = stack.pop()\n",
        "      stack.append(a-b)\n",
        "    elif x == '*':\n",
        "      b = stack.pop()\n",
        "      a = stack.pop()\n",
        "      stack.append(a*b)\n",
        "    elif x == '/':\n",
        "      b = stack.pop()\n",
        "      a = stack.pop()\n",
        "      stack.append(a // b)\n",
        "    else:\n",
        "      stack.append(int(x))\n",
        "    print(stack)\n",
        "\n",
        "  print(stack.pop())\n"
      ],
